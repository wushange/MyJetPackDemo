  **********************************************-->
    <attr name="aui_config_color_blue" format="color" />
    <attr name="aui_config_color_red" format="color" />
    <attr name="aui_config_color_separator" format="color" />
    <attr name="aui_config_color_separator_darken" format="color" />
    <attr name="aui_config_color_background" format="color" />
    <attr name="aui_config_color_background_pressed" format="color" />
    <attr name="aui_config_color_black" format="color" />
    <attr name="aui_config_color_link" format="color" />
    <attr name="aui_config_color_pressed" format="color" />

    <attr name="aui_config_color_gray_1" format="color" />
    <attr name="aui_config_color_gray_2" format="color" />
    <attr name="aui_config_color_gray_3" format="color" />
    <attr name="aui_config_color_gray_4" format="color" />
