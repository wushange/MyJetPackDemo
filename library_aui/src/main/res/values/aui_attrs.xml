<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="aui_content_padding_horizontal" format="dimension" /> <!-- 已废弃 -->
    <attr name="aui_content_spacing_horizontal" format="dimension" /> <!-- margin 和 padding 等使用的内容通用水平间距 -->
    <!--**********************************************
